@{
    ViewData["Title"] = "Employee Management";
}

<!--Page Title-->
<section class="page-title" style="background-image:url(../../images/banner-images/document.png)">
    <div class="auto-container">
        <div class="content">
            <h1>@ViewData["Title"]</h1>
            <ul class="page-breadcrumb">
                <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                <li>@ViewData["Title"]</li>
            </ul>
        </div>
    </div>
</section>
<!--End Page Title-->
<!-- Services Section Three-->
<section class="services-section-three">
    <div class="auto-container">
        <div class="row clearfix">

            <!-- Services Block -->
            <div class="services-block-three style-two col-lg-4 col-md-6 col-sm-12">
                <a asp-controller="Employee" asp-action="List">
                    <div class="inner-box wow fadeIn" data-wow-delay="0ms" data-wow-duration="1500ms">
                        <div class="icon-box">
                            <span class="icon flaticon-employee"></span>
                        </div>
                        <h6>Manage Employees</h6>
                        <div class="text">Manage employees' role and information</div>
                    </div>
                </a>
            </div>
        </div>
    </div>
</section>
