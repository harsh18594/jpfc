<!--End Main Slider-->
<!-- About Section Four -->
<section class="about-section-four">
    <div class="auto-container">
        <!-- Sec Title Three -->
        <div class="sec-title-three">
            <div class="clearfix">
                <div class="pull-left">
                    <h2>About us</h2>
                </div>
                <div class="pull-right">
                    <div class="text">
                        <h6><strong>Vision:</strong> Help ordinary people to achieve extra-ordinary dreams!</h6>
                        <h6><strong>Mission:</strong> Creative financial plan to add few zeros to your bank account</h6>
                    </div>
                </div>
            </div>
        </div>

        <div class="row clearfix">
            <!-- Content Column -->
            <div class="image-column col-lg-5 col-md-12 col-sm-12">
                <div class="inner-column">
                    <div class="image-1"><img src="~/images/jewel_loanpower-min.png" alt=""></div>
                </div>
            </div>

            <!-- Skills Column -->
            <div class="skills-column col-lg-7 col-md-12 col-sm-12">
                <div class="inner-column">
                    <div class="row clearfix">
                        <!-- Services Block Thirteen -->
                        <div class="services-block-thirteen col-lg-4 col-md-6 col-sm-12">
                            <div class="inner-box">
                                <div class="icon-box">
                                    <span class="icon flaticon-headphones"></span>
                                </div>
                                <h5>Within 24 hours <br> <span class="text-muted f-1rem">Response</span></h5>
                            </div>
                        </div>
                        <!-- Services Block Thirteen -->
                        <div class="services-block-thirteen col-lg-4 col-md-6 col-sm-12">
                            <div class="inner-box">
                                <div class="icon-box alternate">
                                    <span class="icon flaticon-loading"></span>
                                </div>
                                <h5>Since 1989 <br> <span class="text-muted f-1rem">Experience</span></h5>
                            </div>
                        </div>
                        <!-- Services Block Thirteen -->
                        <div class="services-block-thirteen col-lg-4 col-md-12 col-sm-12">
                            <div class="inner-box">
                                <div class="icon-box">
                                    <span class="icon flaticon-creativity"></span>
                                </div>
                                <h5>Creative <br> <span class="text-muted f-1rem">Finance Idea</span></h5>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
